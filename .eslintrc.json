{
    "extends": [
        "@trevorblades/eslint-config/react",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "custom-rules"
    ],
    "overrides": [
        {
            "files": [
                "**/__tests__/*.js",
                "**/__tests__/*.jsx",
                "**.test.js"
            ],
            "env": {
                "jest": true
            }
        }
    ]
}